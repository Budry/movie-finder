  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Budry/movie-finder.git"
  },
  "author": "Ondřej Zráuba <info@zaruba-ondrej.cz> (https://zaruba-ondrej.cz)",
  "license": "MIT",
  "homepage": "https://github.com/Budry/movie-finder#readme",
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@types/react": "^16.3.10",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.16",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "electron-packager": "^12.0.1",
    "node-sass": "^4.8.3",
    "typescript": "^2.8.1"
  },
  "build": {
